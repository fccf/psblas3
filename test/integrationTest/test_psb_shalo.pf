		check(2)=18
		check(3)=1
		check(4)=8
		check(5)=8
		check(6)=32
		check(7)=32

	endif
	if (me == 4) then
		check(1)=32
		check(2)=32
		check(3)=8
		check(4)=18
		check(5)=18
	endif

        ! END OF SETUP

	call psb_barrier(icontxt)

	!We can do something better here
        x%v%v = x%v%v*2*me

        if (allocated(vg)) deallocate(vg)
        if (allocated(ia)) deallocate(ia)
        if (allocated(val)) deallocate(val)

